{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height": 500,
    "width": 500,
    "description": "bruh",
    "data": {
      "url": "data/Data2.csv"
    },
    "mark": "point",
    "encoding": {
      "x": {
        "field": "Generation (TWh)",
        "type": "quantitative",
        "scale": {"zero": false}
      },
      "y": {
        "field": "Change on last year (TWh)",
        "type": "quantitative",
        "scale": {"zero": false}
      },
      "color": {"field": "Area", "type": "nominal"},
      "shape": {"field": "Variable", "type": "nominal"}
    }
  }
  